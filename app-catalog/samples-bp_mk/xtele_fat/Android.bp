android_app {
    name: "XTeleFat",

    certificate: "platform",
    platform_apis: true,
    system_ext_specific: true,

    optimize: {
        enabled: false,
    },

    // 引入库会使编译产物变得很“臃肿”，但是却可以用里面的类
    static_libs: [
        "Settings-core",
    ],

    libs: [
        "telephony-common",
        "extphonelib",
    ],

    srcs: ["src/**/*.java"],
}
